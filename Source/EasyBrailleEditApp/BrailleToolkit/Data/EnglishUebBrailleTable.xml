  <symbol text="." code="32" name="full stop" />
  <symbol text="," code="02" name="comma" />
  <symbol text="?" code="26" name="question mark" />
  <symbol text="'" code="04" name="apostrophe" />
  <symbol text="(" code="2036" name="opening parenthesis" />
  <symbol text=")" code="3604" name="closing parenthesis" />

  <!-- Special Marks -->
  <symbol text="CapitalSign" code="20" type="Mark" />
  <symbol text="NumberSign" code="3C" type="Mark" />

